{
    "name": "fernand197/phpmini",
    "type": "project",
    "description": "The PHPMini micro-framework",
    "keywords": ["PHPMini", "framework"],
    "require": {
        "phpmailer/phpmailer": "^6.5",
        "symfony/var-dumper": "^5.4",
        "vlucas/phpdotenv": "^5.4",
      "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\": "database/",
            "PHPMini\\": "framework/"
        },
        "files": [
            "framework/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [{
        "name": "Fernand Tatchum",
        "email": "tatchumf@gmail.com"
    }],
    "require-dev": {
      "phpunit/phpunit": "9.5"
    }
}
